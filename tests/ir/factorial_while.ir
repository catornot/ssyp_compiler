#F_BEGIN;
    #F_NAME factorial_while;
    #F_ARGS_BEGIN;
        @Int x;
    #F_ARGS_END;
    #F_VARS_BEGIN;
        @Int k;
        @Int result;
    #F_VARS_END;
    #F_BODY_BEGIN;
        #= k #1;
        #= result #1;
        #WHILE <= k x;
            #* result result k;
            #+ k k #1;
        #ENDWHILE;
        #F_RETURN result;
    #F_BODY_END;
#F_END;

#F_BEGIN;
    #F_NAME main;
    #F_VARS_BEGIN;
        @Int result;
    #F_VARS_END;
    #F_BODY_BEGIN;
        #factorial_while result #7;
        #F_RETURN result;
    #F_BODY_END;
#F_END;